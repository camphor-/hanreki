{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": [
    {
      "type": "object",
      "properties": {
        "title": {"type": "string"},
        "make_title": {
          "anyOf": [
            {"type": "string"},
            {"type": "null"}
          ]
        },
        "start": {
          "type": "string",
          "format": "date-time"
        },
        "end": {
          "type": "string",
          "format": "date-time"
        },
        "url": {
          "anyOf": [
            {"type": "string"},
            {"type": "null"}
          ]
        },
        "make_start": {
          "anyOf": [
            {"type": "string", "format": "date-time"},
            {"type": "null"}
          ]
        },
        "make_end": {
          "anyOf": [
            {"type": "string", "format": "date-time"},
            {"type": "null"}
          ]
        }
      },
      "required": ["title", "start", "end", "url"]
    }
  ]
}
